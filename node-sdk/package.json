{
  "name": "@januscaler/node-sdk-js",
  "version": "1.0.0",
  "description": "januscaler client sdk for javascript",
  "author": "Shivansh Talwar <shivanshtalwar0@gmail.com>",
  "keywords": [
    "typescript",
    "cli",
    "starter"
  ],
  "license": "MIT",
  "typings": "dist/",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "nodemon": "nodemon -e ts --watch src/  --exec \"npm run build\"",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "npm run clean && tsc",
    "test": "npm run build && ava --verbose",
    "bundle": "npm run build && pkg  . --out-dir ./exec/"
  },
  "devDependencies": {
    "ava": "^5.0.1",
    "nodemon": "^2.0.20",
    "pkg": "^5.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/lodash": "^4.14.186",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "aigle": "^1.14.1",
    "commander": "^2.20.0",
    "lodash": "^4.17.21",
    "rxjs": "^7.5.7",
    "typed_janus_js": "^1.0.13",
    "uuid": "^9.0.0",
    "ws": "^8.9.0"
  }
}
